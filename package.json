{
  "name": "elm-webpack",
  "version": "0.1.0",
  "description": "Elm Webpack starter",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server --hot --inline\"",
    "build": "cross-env BABEL_ENV=production npm run clean && webpack -p --bail",
    "dev": "cross-env BABEL_ENV=development webpack",
    "clean": "rm -rf dist",
    "elm": "elm",
    "elm-repl": "elm repl",
    "elm-package": "elm package",
    "elm-reactor": "elm reactor",
    "elm-make": "elm make",
    "elm-test": "elm test",
    "postinstall": "elm package install -y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Ilyes512@github.com/Ilyes512/elm-webpack.git"
  },
  "keywords": [
    "elm",
    "webpack",
    "starter",
    "boilerplate"
  ],
  "author": "Ilyes512",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ilyes512/elm-webpack/issues"
  },
  "homepage": "https://github.com/Ilyes512/elm-webpack#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "bulma": "^0.5.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^5.6.0",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
